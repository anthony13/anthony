--pembatas

length' x = x

--pembatas

reverse' x = x

--pembatas

last' (a) = a

--pembatas

tail' (x:xs) = xs

--pembatas

init' x = x

--pembatas

max' a b
  |a >= b = a
  |a <= b = b

--pembatas

min' a b
  |a <= b = a
  |a >= b = b

--pembatas

