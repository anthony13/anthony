module One where

import Data.List

--- fst, snd, null, head, tail, take, drop, max, min
null' [] = True
null' (x:xs) = False
-- null true nya himpunan kosong
-- sisanya salah
--pembatas

take' 0 (x:xs) = []
take' a [] = []
take' a (x:xs) = x : take' (a-1) xs

-- take 2 [1,2,3,4] = [1,2]()
-- : 1 : [2,3] = [1,2,3]

--pembatas

drop' 0 (x:xs) = (x:xs)
drop' a [] = []
drop' a (x:xs) = drop' (a-1) xs

--pembatas

fst' (a, b) = a

--pembatas

snd' (a, b) = b

--pembatas

map' x = x


--pembatas

filter' x = x

--pembatas

delete' x = x

--pembatas

deleteAll' x = x

--pembatas

